{
  "name": "research-rss-reader",
  "description": "A basic rss reader that can track which items are read and unread, and tag items.",
  "version": "0.1.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "deploy": "npm run export && node ./scripts/gh-pages.js",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "fix": "eslint --fix src/**/*.{js,svelte}",
    "format": "prettier --write src/**/*.{js,svelte}"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "idb": "^5.0.4",
    "lodash": "^4.17.19",
    "polka": "next",
    "rss-parser": "^3.8.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "sapper": "^0.27.0",
    "svelte": "^3.49.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  }
}
